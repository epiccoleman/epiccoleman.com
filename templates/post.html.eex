<header>
epiccoleman.com
</header>
<h1><a href="<%= base() %>"><%= @site.name %></a></h1>
<p><%= @site.description %></p>
<%= include "nav" %>
<h1><%= @page.title %></h1>
<p>Posted on <%= @page.date %> by <%= @site.author %></p>
<%= unless Enum.empty?(@page.tags) do %>
  <p>Tags:</p>
  <ul>
    <%= for t <- @page.tags do %>
      <li><a href="<%= t.list_url %>"><%= t.name %></a></li>
    <% end %>
  </ul>
<% end %>
<%= @contents %>
